{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.s",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": [
            "GET",
            "POST", 
            "PUT", 
            "DELETE"
        ]
      }
    ]
  }